{
    "redirects": [
      { "source": "/api/top100", "destination": "/https://api-server-7y33.vercel.app/api/top100" },
      { "source": "/user", "destination": "/api/user", "permanent": false },
      { "source": "/view-source", "destination": "https://github.com/vercel/vercel" },
      {
        "source": "/:path((?!uk/).*)",
        "has": [
          {
            "type": "header",
            "key": "x-vercel-ip-country",
            "value": "GB"
          }
        ],
        "destination": "/uk/:path*",
        "permanent": false
      }
    ]
  }
